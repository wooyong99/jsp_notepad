<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<jsp:useBean id="customer" class="com.exam_usebean.Customer"></jsp:useBean>
<!DOCTYPE html>
<html>
<head>
<%
request.setCharacterEncoding("UTF-8)
String name = request.getParameter("name");
String id = request.getParameter("id");
String pw = request.getParameter("pw");
%>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<jsp:setProperty property="name" name="customer" value='<%=name %>'/>
	<jsp:setProperty property="id" name="customer" value='<%=id %>'/>
	<jsp:setProperty property="pw" name="customer" value='<%=pw %>'/>
	<jsp:setProperty property="birth" name="customer" value="gg"/>
	<jsp:setProperty property="address" name="customer" value="gg"/>
	<jsp:setProperty property="email" name="customer" value="gg"/>
	<jsp:getProperty property="id" name="customer" />
	<jsp:getProperty property="pw" name="customer" />
	<jsp:getProperty property="name" name="customer" />
	<jsp:getProperty property="address" name="customer" />
	<jsp:getProperty property="email" name="customer" />
</body>
</html>